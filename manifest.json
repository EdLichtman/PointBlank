{
  "name": "blankYourAnchors",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Inject _blank into your anchor tags",
  //"homepage_url": "",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab"
  ]
}